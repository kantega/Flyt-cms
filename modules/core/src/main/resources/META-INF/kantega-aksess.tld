            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Only get pages published in specified category (column)</description>
            <name>associationcategory</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Get pages with specified language, default is language of current page</description>
            <name>language</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>int</type>
        </attribute>
        <attribute>
            <description>Deprecated, use rootid</description>
            <name>rootid</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Specify when start of sitemap should not be homepage of site</description>
            <name>startid</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Get sitemap of specified site, default is site from current page</description>
            <name>siteid</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Maximum levels (depth) of pages to get</description>
            <name>depth</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>int</type>
        </attribute>
        <attribute>
            <description>Includes current page in sitemap even when published in other associationcategory</description>
            <name>alwaysincludecurrentid</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <attribute>
            <description>Set to true to get all pages, ignoring language</description>
            <name>ignorelanguage</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <example>
            <![CDATA[
                    We have a site which contains information in different languages: Norwegian and English. The English start page
                    has the alias "/english/.


                    Fetch and print sitemap for Norwegian pages (max 3 levels):

                        <aksess:getsitemap name="sitemap" depth="3" associationcategory="Global menu"/>
                        <aksess:printsitemap name="sitemap"/>


                    Fetch and print sitemap for English pages (max 3 levels):

                        <aksess:getsitemap name="sitemap" depth="3" rootid="/english/ associationcategory="Global menu"/>
                        <aksess:printsitemap name="sitemap"/>


                    Fetch and print sitemap for whole site:

                        <aksess:getsitemap name="sitemap" depth="3" ignorelanguage="true" associationcategory="Global menu"/>
                        <aksess:printsitemap name="sitemap"/>
            ]]>
        </example>
