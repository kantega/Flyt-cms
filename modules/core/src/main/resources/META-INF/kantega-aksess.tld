    <tag>
        <description>Stores the specified attribute for current page (or page specified using collection or obj) as request attribute</description>
        <name>setvariable</name>
        <tag-class>no.kantega.publishing.api.taglibs.content.SetVariableTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>Name of page (content/metadata) attribute to save as request attribute</description>
            <name>attribute</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Attribute type to check if exists: default is content data, specifiy metadata for metadata attributes</description>
            <name>attributetype</name>
            <required>false</required>
