<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
        version="2.0">
    <description>Flyt CMS Main Taglib API</description>
    <tlib-version>2.0</tlib-version>
    <short-name>aksess</short-name>
    <uri>http://www.kantega.no/aksess/tags/aksess</uri>
    <tag>
        <description>Prints value from config file </description>
        <name>getconfig</name>
        <tag-class>no.kantega.publishing.api.taglibs.util.GetConfigTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>Configuration value to get from Aksess config file</description>
            <name>key</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Default value is no value is present in Aksess config file</description>
            <name>default</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <description>Print today's date</description>
        <name>getdate</name>
        <tag-class>no.kantega.publishing.api.taglibs.util.GetDateTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>Date format, using SimpleDateFormat</description>
            <name>format</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <description>Print URL as absolute URL</description>
        <name>geturl</name>
        <tag-class>no.kantega.publishing.api.taglibs.util.GetUrlTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>URL to print (prints URL to startpage if url is not specified)</description>
            <name>url</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Specifies whether to add context path to start of url or not. default is 'true'</description>
            <name>addcontextpath</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Specifies whether to generate an absolute url, with scheme, domain and port(if not 80 or 443). default is 'false'</description>
            <name>absoluteUrl</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
            <description>Extra query parameters which should be added to the generated URL</description>
            <name>queryparams</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Url have escaped &amp;s by default. Set to false to disable</description>
            <name>escapeurl</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>

        <example>
            <![CDATA[
                    Print URL to root:
                        <aksess:geturl/> -> /contextpath
                        <aksess:geturl url="/someresource"/> -> /contextpath/someresource
                        <aksess:geturl url="/someresource"/ absoluteUrl="true"> -> http(s)://server(:8080)/contextpath/someresource
            ]]>
        </example>
    </tag>
    <tag>
        <description>Fetch and print HTML content from external URL</description>
        <name>gethtml</name>
        <tag-class>no.kantega.commons.taglib.util.GetHtmlTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>URL of web page to fetch</description>
            <name>url</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <description>Include JSP file replacing macros in URL.</description>
        <name>include</name>
        <tag-class>no.kantega.publishing.api.taglibs.util.IncludeTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>Path to JSP file to include. Macros are $SITE = site of current page and $LANGUAGE language of current page</description>
            <name>url</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <example>
            <![CDATA[
                    Include page "design.jsp" replacing $SITE with alias of current site:
                        <aksess:include url="/WEB-INF/jsp$SITE/include/design.jsp"/>
            ]]>
        </example>
    </tag>
    <tag>
        <description>Check if attribute equals specified value and print body if is equal</description>
        <name>ifequals</name>
        <tag-class>no.kantega.publishing.api.taglibs.content.AttributeIfEqualsTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>Name of page attribute</description>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Value to compare page attribute with</description>
            <name>value</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Name of collection to find page attribute in, used with getcollection tag</description>
            <name>collection</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Id or alias of other page to get attribute from</description>
            <name>contentid</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Inverts statement, eg check if attribute is NOT equal to specified value</description>
            <name>negate</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <attribute>
            <description>Searches parents of page for attribute if attribute does not exists in this page</description>
            <name>inheritfromancestors</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <example>
            <![CDATA[
                    Show body if attribute "fruit" has value "apple":
                        <aksess:ifequals name="fruit" value="apple">
                            ...
                        </aksess:ifequals>
            ]]>
        </example>
    </tag>
    <tag>
        <description>Check if alias of current page equals specified alias and print body if is equal</description>
        <name>ifaliasequals</name>
        <tag-class>no.kantega.publishing.api.taglibs.content.AttributeIfAliasEqualsTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>Value to compare page alias with</description>
            <name>value</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Inverts statement, eg check if alias is NOT equal to specified value</description>
            <name>negate</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <attribute>
            <description>Name of collection to find page attribute in, used with getcollection tag</description>
            <name>collection</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Id or alias of other page to get attribute from</description>
            <name>contentid</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <description>Check if attribute contains specified value. Typically used with attributes which can contains several values</description>
        <name>ifcontains</name>
        <tag-class>no.kantega.publishing.api.taglibs.content.AttributeIfContainsTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>Name of page attribute</description>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Value to compare page attribute with</description>
            <name>value</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Name of collection to find page attribute in, used with getcollection tag</description>
            <name>collection</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Id or alias of other page to get attribute from</description>
            <name>contentid</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Searches parents of page for attribute if attribute does not exists in this page</description>
            <name>inheritfromancestors</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <attribute>
            <description>Specifies a no.kantega.publishing.common.data.Content to use directly</description>
            <name>obj</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>no.kantega.publishing.common.data.Content</type>
        </attribute>
    </tag>
    <tag>
        <description>Print body if specified attribute exists and is not empty</description>
        <name>exists</name>
        <tag-class>no.kantega.publishing.api.taglibs.content.AttributeExistsTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>Name of page attribute to check if exists</description>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Name of collection to find page attribute in, used with getcollection tag</description>
            <name>collection</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Id or alias of other page to get attribute from</description>
            <name>contentid</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Name of the repeater containing the attribute, use with repeatattributes tag</description>
            <name>repeater</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Inverts statement, eg check if attribute not exists</description>
            <name>negate</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <attribute>
            <description>Attribute type to check if exists: default is content data, specifiy metadata for metadata attributes</description>
            <name>attributetype</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Searches parents of page for attribute if attribute does not exists in this page</description>
            <name>inheritfromancestors</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <attribute>
            <description>Specifies a no.kantega.publishing.common.data.Content to use directly</description>
            <name>obj</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>no.kantega.publishing.common.data.Content</type>
        </attribute>
        <example>
            <![CDATA[
                    Display attribute "text" if it exists:

                        <aksess:exists name="text">
                            <div>
                                <aksess:getattribute name="text"/>
                            </div>
                        </aksess:exists>


                    Display attribute "text" from collection "news" if it exists:

                        <aksess:getcollection name="news" associatedid="/news/">
                            <aksess:exists name="text" collection="news">
                                <div>
                                    <aksess:getattribute name="text" collection="news"/>
                                </div>
                            </aksess:exists>
                        </aksess:getcollection>
            ]]>
        </example>
    </tag>
    <tag>
        <description>Print body if specified attribute not exists or is empty</description>
        <name>notexists</name>
        <tag-class>no.kantega.publishing.api.taglibs.content.AttributeNotExistsTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>Name of page attribute to check if NOT exists</description>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Name of collection to find page attribute in, used with getcollection tag</description>
            <name>collection</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Id or alias of other page to get attribute from</description>
            <name>contentid</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Name of a repeater, use with repeatattributes tag</description>
            <name>repeater</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Attribute type to check if exists: default is content data, specifiy metadata for metadata attributes</description>
            <name>attributetype</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Searches parents of page for attribute if attribute does not exists in this page</description>
            <name>inheritfromancestors</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <attribute>
            <description>Specifies a no.kantega.publishing.common.data.Content to use directly</description>
            <name>obj</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>no.kantega.publishing.common.data.Content</type>
        </attribute>
        <example>
            <![CDATA[
                    Display default text if attribute "text" from collection "news" has no value:

                        <aksess:getcollection name="news" associatedid="/news/">
                            <aksess:notexists name="text" collection="news">
                                <div>
                                    ...default text...
                                </div>
                            </aksess:notexists>
                        </aksess:getcollection>
            ]]>
        </example>
    </tag>
    <tag>
        <description>Prints body if link is external</description>
        <name>isexternallink</name>
        <tag-class>no.kantega.publishing.api.taglibs.content.IsExternalLinkTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>Id or alias of other page to link to</description>
            <name>contentid</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Name of collection to find page to link to, used with getcollection tag</description>
            <name>collection</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Inverts statement, eg check if link is not external</description>
            <name>negate</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>

    </tag>
    <tag>
        <description>Print body if current page is within path of specified contentid</description>
        <name>isinpath</name>
        <tag-class>no.kantega.publishing.api.taglibs.content.IsInPathTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>Id or alias of other page</description>
            <name>contentid</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>ContentObject who owns the path. if omitted, current page is used.</description>
            <name>obj</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Inverts statement, eg check if contentId is NOT in path</description>
            <name>negate</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <description>Prints the specified attribute for current page (or page specified using collection or obj)</description>
        <name>getattribute</name>
        <tag-class>no.kantega.publishing.api.taglibs.content.GetAttributeTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>Name of page attribute to print</description>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Name of collection to print page attribute from, used with getcollection tag</description>
            <name>collection</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Specify custom CSS class to use when tag return a HTML tag such as an IMG tag</description>
            <name>cssclass</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Transform result to uppercase or lowercase</description>
            <name>transform</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Id or alias of other page to print attribute from</description>
            <name>contentid</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Name of a repeater, use with repeatattributes tag</description>
            <name>repeater</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>For DateAttribute specifies date format, using SimpleDateFormat</description>
            <name>format</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Specify page attribute property to print: html (default), value, url, width (for images), height (for images), parentid (for images), description (for images)</description>
            <name>property</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
            <description>Used with image attributes, sets maximum height (image will be scaled if necessary)</description>
            <name>height</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>int</type>
        </attribute>
        <attribute>
            <description>Used with image attributes, sets maximum width (image will be scaled if necessary)</description>
            <name>width</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>int</type>
        </attribute>
        <attribute>
            <description>Used with text attributes, sets maximum length of text (text will be cut if longer)</description>
            <name>maxlength</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>int</type>
        </attribute>
        <attribute>
            <description>Specifies a default value if attribute does not exists</description>
            <name>defaultvalue</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Searches parents of page for attribute if attribute does not exists in this page</description>
            <name>inheritfromancestors</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>boolean</type>
        </attribute>
        <attribute>
            <description>Used with images, determines if links to images should show image in browser (inline) or download (attachment)</description>
            <name>contentdisposition</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>Specifies a no.kantega.publishing.common.data.Content to use directly</description>
            <name>obj</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>no.kantega.publishing.common.data.Content</type>
        </attribute>
        <attribute>
            <description>Used with images. Contain is default, and ensures the whole image is contained within values supplied. Center is center weighted cropping</description>
            <name>cropping</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>no.kantega.publishing.common.data.enums.Cropping</type>
        </attribute>
        <example>
            <![CDATA[
                    Get attribute with name "text":
                        <aksess:getattribute name="text"/>


                    Get attribute with name "phonenumber" from frontpage:
                        Phonenumber: <aksess:getattribute name="phonenumber" contentid="/"/>

                    Get attribute with name "picture" and shrink it if wider than 100 pixels:
                        <aksess:getattribute name="picture" width="100"/>

                    Get attribute with name "picture" and shrink it to just cover an area of 100 by 100 pixels, cropping from center of image
                        <aksess:getattribute name="picture" width="100" height="100" cropping="CENTERED"/>


                    Get width of image attribute with name "picture":
                        <aksess:getattribute name="picture" property="width"/>

            ]]>
        </example>
    </tag>
